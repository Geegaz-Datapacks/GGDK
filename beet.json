{
  "author": "Geegaz",
  "description": "A datapack about drinks & alcohol",
  "name": "GGDK",
  "version": "2.1.0",
  "data_pack": {
    "load": [
      "src"
    ],
    "pack_format": 59
  },
  "resource_pack": {
    "load": [
      "src"
    ],
    "pack_format": 45
  },
  "require": [
    "beet.contrib.extra_files"
  ],
  "pipeline": [
    "beet.contrib.installation_advancement",
    "beet.contrib.babelbox"
  ],
  "meta": {
    "installation_advancement": {
      "project_advancement_path": "ggdk:installed",
      "icon": {
        "id": "minecraft:knowledge_book",
        "components": {
          "item_model": "ggdk:wine"
        }
      }
    },
    "babelbox": {
      "load": [
        "translations/ggdk.csv"
      ]
    },
    "extra_files": {
      "data_pack": [
        "LICENSE_datapack.md"
      ],
      "resource_pack": [
        "LICENSE_resourcepack.md",
        "assets/ggdk/items/beer.json",
        "assets/ggdk/items/chocolate.json",
        "assets/ggdk/items/cider.json",
        "assets/ggdk/items/tea.json",
        "assets/ggdk/items/tequila.json",
        "assets/ggdk/items/vodka.json",
        "assets/ggdk/items/whiskey.json",
        "assets/ggdk/items/wine.json",
        "assets/ggdk/items/bar_stool.json",
        "assets/ggdk/items/small_table.json",
        "assets/ggdk/items/neon_tube.json"
      ]
    }
  }
}