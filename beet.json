{
  "author": "Geegaz",
  "description": "A datapack about drinks & alcohol",
  "name": "GGDK",
  "version": "2.1.1",
  "data_pack": {
    "load": [
      "src"
    ],
    "pack_format": 59
  },
  "resource_pack": {
    "load": [
      "src"
    ],
    "pack_format": 45
  },
  "require": [
    "beet.contrib.extra_files",
    "beet.contrib.unknown_files",
    "plugins.gsheets_translations"
  ],
  "pipeline": [
    "beet.contrib.installation_advancement",
    "beet.contrib.babelbox"
  ],
  "meta": {
    "installation_advancement": {
      "project_advancement_path": "ggdk:installed",
      "icon": {
        "id": "minecraft:knowledge_book",
        "components": {
          "item_model": "ggdk:wine"
        }
      }
    },
    "gsheets_translations": {
      "sheet_id": 0,
      "spreadsheet_id": "144mhHsIzvqovH30cmGMgiHWHP_Fb4huwsOg8bK4EghE",
      "output": "ggdk.csv"
    },
    "babelbox": {
      "load": [
        ".beet_cache/translations/ggdk.csv"
      ]
    },
    "extra_files": {
      "data_pack": [
        "LICENSE_datapack.md"
      ],
      "resource_pack": [
        "LICENSE_resourcepack.md"
      ]
    }
  }
}